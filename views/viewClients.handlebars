<section class="content">
    <div class="block-header">
        <div class="row">
            <div class="col-lg-7 col-md-6 col-sm-12">
                <h2>View Clients
                <small class="text-muted">Welcome to Promoworks Application</small>
                </h2>
            </div>
            <div class="col-lg-5 col-md-6 col-sm-12">
                <ul class="breadcrumb float-md-right">
                    <li class="breadcrumb-item"><a href="index.html"><i class="zmdi zmdi-home"></i> Promoworks</a></li>
                    <li class="breadcrumb-item"><a href="javascript:void(0);">Clients</a></li>
                    <li class="breadcrumb-item active">View Clients</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="card">
                    <div class="header">
                        <ul class="header-dropdown">
                            <li class="dropdown"> <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"> <i class="zmdi zmdi-more-vert"></i> </a>
                                <ul class="dropdown-menu pull-right">
                                    <li><a href="javascript:void(0);">Action</a></li>
                                    <li><a href="javascript:void(0);">Another action</a></li>
                                    <li><a href="javascript:void(0);">Something else here</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <div class="body">
                        <div class="table-responsive">
                            <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                                <thead>
                                    <tr>
												<th>Client Id</th>
												<th>Client Name</th>
												<th>User Name</th>
												<th>Office</th>
												<th>Mobile No</th>
												<th>Action</th>
												<th>E/D</th>
												<th>OTP E/D</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
												<th>Client Id</th>
												<th>Client Name</th>
												<th>User Name</th>
												<th>Office</th>
												<th>Mobile No</th>
												<th>Action</th>
												<th>E/D</th>
												<th>OTP E/D</th>
                                    </tr>
                                </tfoot>
                                <tbody>
                                                                                                          {{#each Clientuser}}
											<tr>
												<td>{{this._id}}</td>
												<td>{{this.clientName}}</td>
												<td>{{this.username}}</td>
												<td>{{this.clientOffice}}</td>
												<td>{{this.mobile}}</td>

                                                <td style="width: 50px;" class="jsgrid-cell jsgrid-control-field jsgrid-align-center"><a href="/clients/updateClients/{{this._id}}/edit" ><span class="zmdi zmdi-edit" type="button" title="Edit"></span></a> | <a href="/clients/Clientuser/{{this._id}}"><span class="zmdi zmdi-delete" type="button" title="Delete"></span></a></td>   
                                                <td style="width: 50px;" class="jsgrid-cell jsgrid-control-field jsgrid-align-center">
                                                      {{#if Disable}}
                                                    <a href="/clients/EnableClients/{{this._id}}">E</a> 
                                                            {{/if}}
                                                        {{#if Enable}}

                                                    <a href="/clients/DisableClients/{{this._id}}">D</a></td>
                                                
                                                {{/if}}                        
                                                
                                                <td style="width: 50px;" class="jsgrid-cell jsgrid-control-field jsgrid-align-center">
                                                        {{#if DisableOTP}}
                                                            <a href="/clients/EnableOTPClients/{{this._id}}">E</a> 
                                                        {{/if}}
                                                    
                                                        {{#if EnableOTP}}
                                                            <a href="/clients/DisableOTPClients/{{this._id}}">D</a></td>
                                                       {{/if}}
            
											</tr>
                                              {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
 
    </div>
</section>